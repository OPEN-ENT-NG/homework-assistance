{
  "name": "Homework-assistance",
  "version": "2.0.1-dev.0",
  "type": "module",
  "description": "Homework-assistance",
  "private": true,
  "scripts": {
    "build": "vite build",
    "clean": "concurrently \"pnpm:clean:*\"",
    "clean:dist": "rm -rf dist",
    "clean:lighthouse": "rm -rf .lighthouseci",
    "clean:modules": "rm -rf node_modules",
    "clean:package": "rm -f pnpm-lock.yaml && rm -f package.json",
    "dev": "vite",
    "test": "vitest",
    "fix": "eslint --fix .",
    "check-types": "tsc --noEmit",
    "format": "pnpm format:check && pnpm format:write",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "pnpm clean:lighthouse && lhci autorun",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "preview": "vite preview",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run fix"
    ]
  },
  "dependencies": {
    "@cgi-learning-hub/icons": "develop",
    "@cgi-learning-hub/theme": "develop",
    "@cgi-learning-hub/ui": "develop",
    "@edifice.io/bootstrap": "develop",
    "@edifice.io/client": "develop",
    "@edifice.io/react": "develop",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.0",
    "@mui/material": "5.15.0",
    "@mui/x-date-pickers": "7.21.0",
    "@react-spring/web": "9.7.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@splidejs/react-splide": "^0.7.8",
    "@tanstack/react-query": "4.36.1",
    "animejs": "3.2.1",
    "dayjs": "1.11.10",
    "i18next": "23.5.1",
    "i18next-http-backend": "2.2.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-hook-form": "7.47.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "13.3.0",
    "react-intersection-observer": "9.5.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.16.0",
    "react-toastify": "^11.0.0",
    "swiper": "10.3.1",
    "uuid": "8.3.2",
    "zustand": "4.4.3"
  },
  "devDependencies": {
    "@axe-core/react": "4.8.0",
    "@eslint/js": "^9.18.0",
    "@tanstack/react-query-devtools": "4.36.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "20.8.5",
    "@types/react": "18.2.28",
    "@types/react-color": "3.0.12",
    "@types/react-dom": "18.2.13",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "clsx": "2.0.0",
    "concurrently": "8.2.1",
    "csstype": "3.1.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "happy-dom": "12.9.1",
    "husky": "8.0.3",
    "i18next-browser-languagedetector": "7.2.1",
    "jsdom": "^25.0.1",
    "lint-staged": "14.0.1",
    "nx": "19.6.0",
    "prettier": "3.0.3",
    "sass": "1.69.5",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.21.0",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6",
    "vitest-dom": "0.1.1"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "20"
  },
  "nx": {}
}
