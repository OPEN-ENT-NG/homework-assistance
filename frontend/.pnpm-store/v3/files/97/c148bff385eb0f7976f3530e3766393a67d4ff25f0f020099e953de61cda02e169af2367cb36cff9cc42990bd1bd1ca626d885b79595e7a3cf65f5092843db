import { Theme } from '../types';
declare const theme: Theme;
declare const muiOptions: {
    colorSchemes: {
        light: {
            palette: {
                text: {
                    primary: string;
                };
            };
        };
    };
    typography: {
        fontFamily: string;
    };
};
declare const muiTheme: import('@mui/material').Theme;
declare const tailwindThemeConfig: {
    extend: {
        colors: {
            primary: import('../types').ColorShades;
            secondary: import('../types').ColorShades;
            grey: Omit<import('../types').ColorShades, "contrastText">;
            red: Omit<import('../types').ColorShades, "darker" | "lighter">;
            blue: Omit<import('../types').ColorShades, "darker" | "lighter">;
            green: Omit<import('../types').ColorShades, "darker" | "lighter">;
            yellow: Omit<import('../types').ColorShades, "darker" | "lighter">;
        };
    };
};
export { muiOptions as imtMuiOptions, muiTheme as imtMuiTheme, tailwindThemeConfig as imtTailwindThemeConfig, theme as imtTheme, };
